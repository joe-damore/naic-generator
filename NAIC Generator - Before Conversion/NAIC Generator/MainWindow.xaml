                        Text="{Binding Path=Course.RequestedInsuranceCreditsPropertyCasualty}"
                        TextWrapping="NoWrap"
                        Grid.Column="6"
                        Grid.Row="14"
                        Margin="5,4,0,3"
                        Height="23"
                        GotKeyboardFocus="OnAutoselectTextboxFocus"
                        LostKeyboardFocus="OnAutoselectTextboxLostFocus"
                        >
                    </TextBox>
                    <TextBox
                        x:Name="tbCreditsRequestedSalesEthics"
                        Text="{Binding Path=Course.RequestedSalesCreditsEthics}"
                        TextWrapping="NoWrap"
                        Grid.Column="5"
                        Grid.Row="15"
                        Margin="0,4,5,3"
                        Height="23"
                        GotKeyboardFocus="OnAutoselectTextboxFocus"
                        LostKeyboardFocus="OnAutoselectTextboxLostFocus"
                        >
                    </TextBox>
                    <TextBox
                        x:Name="tbCreditsRequestedInsuranceEthics"
                        Text="{Binding Path=Course.RequestedInsuranceCreditsEthics}"
                        TextWrapping="NoWrap"
                        Grid.Column="6"
                        Grid.Row="15"
                        Margin="5,4,0,3"
                        Height="23"
                        GotKeyboardFocus="OnAutoselectTextboxFocus"
                        LostKeyboardFocus="OnAutoselectTextboxLostFocus"
                        >
                    </TextBox>
                    <TextBox
                        x:Name="tbCreditsRequestedSalesGeneral"
                        Text="{Binding Path=Course.RequestedSalesCreditsGeneral}"
                        TextWrapping="NoWrap"
                        Grid.Column="5"
                        Grid.Row="16"
                        Margin="0,4,5,3"
                        Height="23"
                        GotKeyboardFocus="OnAutoselectTextboxFocus"
                        LostKeyboardFocus="OnAutoselectTextboxLostFocus"
                        >
                    </TextBox>
                    <TextBox
                        x:Name="tbCreditsRequestedInsuranceGeneral"
                        Text="{Binding Path=Course.RequestedInsuranceCreditsGeneral}"
                        TextWrapping="NoWrap"
                        Grid.Column="6"
                        Grid.Row="16"
                        Margin="5,4,0,3"
                        Height="23"
                        GotKeyboardFocus="OnAutoselectTextboxFocus"
                        LostKeyboardFocus="OnAutoselectTextboxLostFocus"
                        >
                    </TextBox>
                    <TextBox
                        x:Name="tbCreditsRequestedSalesLaws"
                        Text="{Binding Path=Course.RequestedSalesCreditsLaws}"
                        TextWrapping="NoWrap"
                        Grid.Column="5"
                        Grid.Row="17"
                        Margin="0,4,5,3"
                        Height="23"
                        GotKeyboardFocus="OnAutoselectTextboxFocus"
                        LostKeyboardFocus="OnAutoselectTextboxLostFocus"
                        >
                    </TextBox>
                    <TextBox

                    <CheckBox
                        x:Name="cbTimedCourseDiffers"
                        IsChecked="{Binding Path=Course.TimedCreditsDiffer}"
                        Grid.Column="5"
                        Grid.Row="20"
                        Grid.ColumnSpan="2"
                        Margin="0,10,0,0"
                        >
                        Timed Courses Differ
                    </CheckBox>

                    <Label
                        Grid.Column="5"
                        Grid.ColumnSpan="2"
                        Grid.Row="21"
                        HorizontalContentAlignment="Center"
                        Margin="0,18,0,10"
                        >
                        <Label.Style>
                            <Style TargetType="Label">
                                <Setter
                                    Property="Visibility"
                                    Value="Collapsed"
                                    />
                                <Style.Triggers>
                                    <DataTrigger
                                        Binding="{Binding ElementName=cbTimedCourseDiffers, Path=IsChecked}"
                                        Value="True"
                                        >
                                        <Setter
                                            Property="Visibility"
                                            Value="Visible"
                                            />
                                    </DataTrigger>
                                </Style.Triggers>
                            </Style>
                        </Label.Style>
                        <Label.Content>
                            Requested (Timed)
                        </Label.Content>
                    </Label>


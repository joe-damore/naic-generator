                        TextWrapping="NoWrap"
                        Grid.Column="3"
                        Grid.Row="15"
                        Margin="5,4,0,3"
                        Height="23"
                        GotKeyboardFocus="OnAutoselectTextboxFocus"
                        LostKeyboardFocus="OnAutoselectTextboxLostFocus"
                        />
                    <!-- Approved, Sales, General -->
                    <TextBox
                        x:Name="tbCreditsApprovedSalesGeneral"
                        Text="{Binding Path=Course.ApprovedSalesCreditsGeneral}"
                        TextWrapping="NoWrap"
                        Grid.Column="2"
                        Grid.Row="16"
                        Margin="0,4,5,3"
                        Height="23"
                        GotKeyboardFocus="OnAutoselectTextboxFocus"
                        LostKeyboardFocus="OnAutoselectTextboxLostFocus"
                        />
                    <!-- Approved, Insurance, General -->
                    <TextBox
                        x:Name="tbCreditsApprovedInsuranceGeneral"
                        Text="{Binding Path=Course.ApprovedInsuranceCreditsGeneral}"
                        TextWrapping="NoWrap"
                        Grid.Column="3"
                        Grid.Row="16"
                        Margin="5,4,0,3"
                        Height="23"
                        GotKeyboardFocus="OnAutoselectTextboxFocus"
                        LostKeyboardFocus="OnAutoselectTextboxLostFocus"
                        />
                    <!-- Approved, Sales, Laws -->
                    <TextBox
                        x:Name="tbCreditsApprovedSalesLaws"
                        Text="{Binding Path=Course.ApprovedSalesCreditsLaws}"
                        TextWrapping="NoWrap"
                        Grid.Column="2"
                        Grid.Row="17"
                        Margin="0,4,5,3"
                        Height="23"
                        GotKeyboardFocus="OnAutoselectTextboxFocus"
                        LostKeyboardFocus="OnAutoselectTextboxLostFocus"
                        />
                    <!-- Approved, Insurance, Laws -->

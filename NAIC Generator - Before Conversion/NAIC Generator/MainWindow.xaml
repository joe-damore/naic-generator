                        Text="{Binding Path=Course.ApprovedSalesCreditsOther}"
                        TextWrapping="NoWrap"
                        Grid.Column="2"
                        Grid.Row="18"
                        Margin="0,4,5,3"
                        Height="23"
                        GotKeyboardFocus="OnAutoselectTextboxFocus"
                        LostKeyboardFocus="OnAutoselectTextboxLostFocus"
                        />
                    <!-- Approved, Insurance, Other -->
                    <TextBox
                        x:Name="tbCreditsApprovedInsuranceOther"
                        Text="{Binding Path=Course.ApprovedInsuranceCreditsOther}"
                        TextWrapping="NoWrap"
                        Grid.Column="3"
                        Grid.Row="18"
                        Margin="5,4,0,3"
                        Height="23"
                        GotKeyboardFocus="OnAutoselectTextboxFocus"
                        LostKeyboardFocus="OnAutoselectTextboxLostFocus"
                        />
                    <!-- Approved, Sales, Adjuster -->
                    <TextBox
                        x:Name="tbCreditsApprovedSalesAdjuster"
                        Text="{Binding Path=Course.ApprovedSalesCreditsAdjuster}"
                        TextWrapping="NoWrap"
                        Grid.Column="2"
                        Grid.Row="19"
                        Margin="0,4,5,3"
                        Height="23"
                        GotKeyboardFocus="OnAutoselectTextboxFocus"
                        LostKeyboardFocus="OnAutoselectTextboxLostFocus"
                        />
                    <!-- Approved, Insurance, Adjuster -->
                    <TextBox
                        x:Name="tbCreditsApprovedInsuranceAdjuster"
                        Text="{Binding Path=Course.ApprovedInsuranceCreditsAdjuster}"
                        TextWrapping="NoWrap"
                        Grid.Column="3"
                        Grid.Row="19"
                        Margin="5,4,0,3"
                        Height="23"
                        GotKeyboardFocus="OnAutoselectTextboxFocus"
                        LostKeyboardFocus="OnAutoselectTextboxLostFocus"
                        />
                    
                    <TextBox
                        x:Name="tbCreditsRequestedSalesLifeHealth"
                        Text="{Binding Path=Course.RequestedSalesCreditsLifeHealth}"
                        TextWrapping="NoWrap"
                        Grid.Column="5"
                        Grid.Row="13"
                        Margin="0,4,5,3"
                        Height="23"
                        GotKeyboardFocus="OnAutoselectTextboxFocus"
                        LostKeyboardFocus="OnAutoselectTextboxLostFocus"
                        >
                    </TextBox>
                    <TextBox
                        x:Name="tbCreditsRequestedInsuranceLifeHealth"
                        Text="{Binding Path=Course.RequestedInsuranceCreditsLifeHealth}"
                        TextWrapping="NoWrap"
                        Grid.Column="6"
                        Grid.Row="13"
                        Margin="5,4,0,3"
                        Height="23"
                        GotKeyboardFocus="OnAutoselectTextboxFocus"
                        LostKeyboardFocus="OnAutoselectTextboxLostFocus"
                        >
                    </TextBox>
                    <TextBox
                        x:Name="tbCreditsRequestedSalesPropertyCasualty"
                        Text="{Binding Path=Course.RequestedSalesCreditsPropertyCasualty}"
                        TextWrapping="NoWrap"
                        Grid.Column="5"
                        Grid.Row="14"
                        Margin="0,4,5,3"
                        Height="23"
                        GotKeyboardFocus="OnAutoselectTextboxFocus"
                        LostKeyboardFocus="OnAutoselectTextboxLostFocus"
                        >
                    </TextBox>
                    <TextBox
